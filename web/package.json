{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mock": "vite --mode mock",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint:es": "eslint src",
    "lint:es:fix": "eslint src --fix",
    "lint:etc": "prettier src --check",
    "lint:etc:fix": "prettier src --write",
    "lint": "pnpm lint:es && pnpm lint:etc",
    "lint:fix": "pnpm lint:es:fix && pnpm lint:etc:fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:debug": "playwright test --debug",
    "test:all": "vitest run && playwright test"
  },
  "dependencies": {
    "@allwagelab/constants": "workspace:*",
    "@allwagelab/design": "^1.0.0",
    "@allwagelab/react": "^0.0.4",
    "@allwagelab/utils": "workspace:*",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^3.9.1",
    "@tanstack/react-query": "^5.62.8",
    "@vitejs/plugin-react": "^4.3.4",
    "axios": "^1.7.9",
    "js-cookie": "^3.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "^7.0.2",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.3.6",
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "20.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "globals": "^15.12.0",
    "jsdom": "^26.0.0",
    "msw": "^2.7.0",
    "typescript": "5.4.5",
    "vite": "^6.0.1",
    "vitest": "^2.1.8"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
